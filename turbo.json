{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },

    "lint": {},

    "test": {
      "outputs": ["coverage/**"]
    },

    "docker:start": {
      "cache": false
    },

